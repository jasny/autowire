{
    "name": "jasny/autowire",
    "description": "Autowiring for PSR-11 containers",
    "keywords": [],
    "license": "MIT",
    "authors": [
        {
            "name": "Arnold Daniels",
            "email": "arnold@jasny.net",
            "homepage": "http://www.jasny.net"
        }
    ],
    "support": {
        "issues": "https://github.com/jasny/autowire/issues",
        "source": "https://github.com/jasny/autowire"
    },
    "require": {
        "php": ">=7.2.0",
        "psr/container": "^1.0",
        "jasny/reflection-factory": "^1.0"
    },
    "require-dev": {
        "jasny/php-code-quality": "^2.3"
    },
    "autoload": {
        "psr-4": {
            "Jasny\\Autowire\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Jasny\\Autowire\\Tests\\": "tests/"
        }
    },
    "scripts": {
        "test": [
            "phpstan analyse",
            "phpunit",
            "phpcs -p src"
        ]
    },
    "scripts-descriptions": {
        "test": "Run all tests and quality checks"
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true,
        "optimize-autoloader": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
